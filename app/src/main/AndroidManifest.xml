<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android7.acer.learn">

    <application
        android:name=".LearnApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.DropDownActivity" />
        <activity android:name=".ui.DrawActivity" />
        <activity android:name=".ui.SearchActivity" />
        <activity android:name=".ui.ToolsActivity" />
        <activity android:name=".ui.HandlerActivity" />
        <activity android:name=".ui.UIActivity" />
        <activity android:name=".ui.NumberPickerActivity"></activity>
    </application>

</manifest>